{
  "name": "nodejs-apis",
  "version": "0.1.0",
  "description": "APIs for Celebration Time application",
  "main": "dist/index",
  "scripts": {
    "start": "node server/cluster.js",
    "start:dev": "nodemon",
    "build": "npm run clean && tsc && npm run lint",
    "clean": "rimraf ./build",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "mocha -r ts-node/register app/src/**/*.spec.ts",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "pre-commit": "npm run tslint:fix && npm run prettier",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "tslint --project tsconfig.json --fix"
  },
  "author": "Hafeez Syed",
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.141",
    "@types/node": "^12.7.9",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "husky": "^3.0.8",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "node-localstorage": "^1.3.1",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "superagent": "^5.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "swagger-ui": "^3.23.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
